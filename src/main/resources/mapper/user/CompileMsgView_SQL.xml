<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 
<mapper namespace="compileMsgView">
     
	<select id="selectAllMsg" resultType="com.code119.myapp.dto.CompileMsgViewDTO">
		SELECT *
		FROM compileMsg_view
	</select>
    
    <select id="selectTransMsgByMsg" resultType="com.code119.myapp.dto.CompileMsgViewDTO">
		SELECT transMsg
		FROM compileMsg_view
		WHERE message=#{message}
	</select>
	
</mapper>